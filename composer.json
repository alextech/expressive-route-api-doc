{
    "name": "alextech/expressive-route-api-doc",
    "description": "Generate OpenAPI schema doc from expressive routes",
    "type": "library",
    "authors": [
        {
            "name": "Sasha Alex Romanenko",
            "email": "alex@alex-tech-adventures.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-expressive": "^3.2"

    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "RouteApiDoc\\": "src/RouteApiDoc"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RouteApiDocTest\\": "test/RouteApiDocTest/"
        }
    }
}
